<h1>Listing tree_of_lives</h1>

<table>
  <tr>
    <th>Tree sphere name</th>
    <th>Tree path number</th>
    <th>Tree path correspondence</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tree_of_lives.each do |tree_of_life| %>
  <tr>
    <td><%= tree_of_life.tree_sphere_name %></td>
    <td><%= tree_of_life.tree_path_number %></td>
    <td><%= tree_of_life.tree_path_correspondence %></td>
    <td><%= link_to 'Show', tree_of_life %></td>
    <td><%= link_to 'Edit', edit_tree_of_life_path(tree_of_life) %></td>
    <td><%= link_to 'Destroy', tree_of_life, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

